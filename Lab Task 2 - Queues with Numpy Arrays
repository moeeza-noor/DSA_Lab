##Task 1 - Queues using numpy
import numpy as np
class queue:
  def _init_ (self):
    self.list=np.array(5)
  def enqueue(self, data):
    arr=np.append(self, data)
    return arr
  def dequeue(self):
    arr=np.delete(self.list,1)
    return arr
  def front(self):
    arr=self.list[0]
    return arr
  def rear(self):
    arr=self.list[-1]
    return arr
  def length(self):
    arr=len(self.list)
    return arr
  def size(self):
    arr=(len(self.list>5))
    return arr
  def isEmpty(self):
    arr=(len(selflist)==0)
    return arr

q=queue()
q.enqueue(12)
q.enqueue(24)
print(q)
q.dequeue()
print(q)

##Task 2 
from collections import deque
class Deque:
    def __init__(self):
        self.items = []
    def isEmpty(self):
        arr=self.items == []
        return arr
    def addRear(self, item):
        arr=self.items.append(item)
        return arr
    def addFront(self, item):
        arr=self.items.insert(0, item)
        return arr
    def removeFront(self):
        arr=self.items.pop(0)
        return arr
    def removeRear(self):
        arr=self.items.pop()
        return arr
    def size(self):
        return len(self.items)
def main():
  q=Queue()
  print(q)
